#include <stdio.h>

int main() {
	int i,max,min,n,a[50];
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	min=max=a[0];
	for(i=0;i<n;i++)
	{
	    if(a[i]>max)
	    {
	        max=a[i];
	    }
	    if (a[i]<min)
	    {
	        min=a[i];
	    }
	}
	
	printf("minimum=%d\n",min);
	printf("maximum=%d\t",max);
	
	return 0 ;
}
