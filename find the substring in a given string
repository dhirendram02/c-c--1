#include<stdio.h>
int main(){
    char str1 [100],str2[50];
    int l,i,j;
    gets(str1);
    gets(str2);
    for(l+0;str2[l] != '\0' && str2[j] !='\0'; j++)
     for(i=0,j=0;str1 [i]!='\0';j++)
     {
         if(str1[i]==str2[j])
         {
             j++;
         }
         else
         {
             j=0;
         }
     }
     if(j=l)
     {
         printf("%d",i-j+1);
     }
     else{
         printf("not found ");
     }
     return 0;
}