#include <stdio.h>

int main() {
	int arr1[50],arr2[50],arr3[50];
	int i,j=0,k=0,n;
	scanf("%d",&n);
	for(i=0;i<n;i++)
    scanf("%d",&arr1[i]);
	{
	    if(arr1[i]%2==0)
	    {
	        arr2[j]=arr1[i];
	        j++;
	    }
	    else {
	        arr3[k]=arr1[i];
	    k++;
	    }
	    
	}
	for(i=0;i<j;i++)
	{

	    printf("Even number %d\n",arr2[i]);
	}
		for(i=0;i<k;i++)
		{
	   printf("odd number %d\n",arr3[i]);
		}
	
	return 0;
}

