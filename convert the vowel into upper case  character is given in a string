#include <stdio.h>
#include <string.h>
int main() {
    char a[50];
    int i=0;
    scanf("%s",a);
    while(a[i]!='\0')
    {
        if(a[i]>95 && a[i]<122)
        {
            if(a[i]=='a'|| a[i]=='e'|| a[i]=='i'|| a[i]=='o'|| a[i]=='u')
            printf("%c",a[i]-32);

            else
            printf("%c",a[i]);
        }
        i++;
    }
return 0;
}