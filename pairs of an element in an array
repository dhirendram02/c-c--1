#include <stdio.h>
void printallpairs(int arr[],int n){
    for(int i=0;i<n;i++)
    {
        int x = arr[i];
        printf("%d",x);
        for(int j=i+1;j<n;j++)
        {
            int y = arr[j];
            printf("%d",y);
        }
    }
}
int main() {
	int arr[]={ 10,20,30,40,50,60};
	int n = sizeof(arr)/sizeof(int);
printfallpairs(arr,n);

	return 0;
}

